    "List = [1, 2, 3, 4, 5, 6, 7, 8, 9]\n",
    "Odd_Count = 0\n",
    "Even_Count= 0\n",
    "for x in List:\n",
    "        if x%2==0:\n",
    "             Even_Count+=1\n",
    "        else:\n",
    "            Odd_Count+=1\n",
    "print(\"Number of Even numbers :\",Even_Count)\n",
    "print(\"Number of Odd numbers :\",Odd_Count)"
   ]
