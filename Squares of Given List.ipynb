    "def square(x):\n",
    "    return x*x\n",
    "\n",
    "n=int(input('Enter no. of elements of list: '))\n",
    "l1=[]\n",
    "print('Enter elements of list')\n",
    "for i in range(n):\n",
    "    element=int(input())\n",
    "    l1.append(element)\n",
    "\n",
    "l2=list(map(square,l1))\n",
    "print('sample list: ',l1)\n",
    "print('output: ',l2)"
